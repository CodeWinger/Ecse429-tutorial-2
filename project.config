[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/CodeWinger (CodeWinger)
[access "refs/for/refs/*"]
	read = group user/CodeWinger (CodeWinger)
	push = group user/CodeWinger (CodeWinger)
	label-Code-Review = -2..+2 group user/CodeWinger (CodeWinger)
	label-Verified = -1..+1 group user/CodeWinger (CodeWinger)
	rebase = group user/CodeWinger (CodeWinger)
	removeReviewer = group user/CodeWinger (CodeWinger)
	submit = group user/CodeWinger (CodeWinger)
[access "refs/heads/*"]
	read = group user/CodeWinger (CodeWinger)
	create = group user/CodeWinger (CodeWinger)
	push = +force group user/CodeWinger (CodeWinger)
	pushMerge = group user/CodeWinger (CodeWinger)
[access "refs/tags/*"]
	push = group user/CodeWinger (CodeWinger)
	push = +force group user/CodeWinger (CodeWinger)
[project]
	description = Using Gerrit
[receive]
	requireChangeId = true
